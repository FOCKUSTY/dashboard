#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules/next/dist/bin/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules/next/dist/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules/next/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules/next/dist/bin/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules/next/dist/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules/next/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/next@14.2.3_@babel+core@7.28.0_react-dom@18.3.1_react@18.3.1__react@18.3.1_sass@1.89.2/node_modules:/media/fockusty/FOSK/fockusty/projects/thevoid/dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
